{
    "name": "vkoori/laravel-stateless-auth",
    "description": "Stateless authentication for Laravel using JWT (JSON Web Tokens). This package enables secure, token-based authentication for API endpoints.",
    "keywords": [
        "JWT",
        "Auth",
        "Guard"
    ],
    "authors": [
        {
            "name": "Koorosh SafeAshrafi",
            "email": "kooroshsafeashrafi@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1",
        "vkoori/laravel-jwt": "^0.1"
    },
    "require-dev": {
        "laravel/framework": "^8.0|^9.0|^10.0|^11.0|^12.0"
    },
    "autoload": {
        "psr-4": {
            "Vkoori\\JwtAuth\\": "src/"
        }
    },
    "extra": {
      "laravel": {
        "providers": [
          "Vkoori\\JwtAuth\\AuthServiceProvider"
        ]
      }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
